#include <iostream>

using namespace std;

int main()
{

    for(int i=1;i<=6;i++)
    {
        for(int j=1;j<=4;j++){
        if(i<=3){
            if(i==1||i==3||j==1||j==4)
                cout<<"*";
            else
                cout<<" ";
                
        }
        else if(i<=6)
        {
            if(j==1||j==i-2)
                cout<<"*";
            else
                cout<<" ";
        }
        }
        cout<<"\n";
    }

    return 0;
}
